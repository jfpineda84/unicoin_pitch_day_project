<p id="notice"><%= notice %></p>

<h1>Response Types</h1>

<table>
  <thead>
    <tr>
      <th>Type</th>
      <th>Data</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @response_types.each do |response_type| %>
      <tr>
        <td><%= response_type.type %></td>
        <td><%= response_type.data %></td>
        <td><%= link_to 'Show', response_type %></td>
        <td><%= link_to 'Edit', edit_response_type_path(response_type) %></td>
        <td><%= link_to 'Destroy', response_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Response Type', new_response_type_path %>
